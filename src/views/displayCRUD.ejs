<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>getCrud</title>
    <style>
        th, tr, td{
            border: 1px solid black;
        }
    </style>
</head>
<body>
    <h3>Get crud from homeController</h3>
    <table style="width: 100%;">
        <tr>
            <th>Email</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Address</th>
            <th>phonenumber</th>
            <th>Sex</th>
            <th>Role</th>
            <th>Position</th>
            <th>CreateAt</th>
            <th>UpdateAt</th>
        </tr>
        <tr>
            <% for(let i=0; i < dataTable.length; i++) { %>
                <tr>
                  <td><%= dataTable[i].email %></td>
                  <td><%= dataTable[i].firstName %></td>
                  <td><%= dataTable[i].lastName %></td>
                  <td><%= dataTable[i].lastName %></td>
                  <td><%= dataTable[i].phonenumber %></td>
                  
                </tr>
             <% } %>
        </tr>
    </table>
</body>
</html>